<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/lift1Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="lift1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lift1SetReps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="lift1SetReps"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lift1Name" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/at_symbol"
        app:layout_constraintStart_toEndOf="@+id/lift1SetReps"
        app:layout_constraintTop_toBottomOf="@+id/lift1Name" />

    <TextView
        android:id="@+id/lift1Weight"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="lift1Weight"
        app:layout_constraintEnd_toStartOf="@+id/lift1Reset"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/lift1Name" />

    <ImageButton
        android:id="@+id/lift1Complete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="16dp"
        android:onClick="liftCompleted"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_done_black_24dp" />

    <Button
        android:id="@+id/lift1Deets"
        android:layout_width="wrap_content"
        android:layout_height="55dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="16dp"
        android:onClick="liftDetails"
        android:text="@string/details_label"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/lift1Complete"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lift2Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:text="lift2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lift1SetReps" />

    <TextView
        android:id="@+id/lift2SetReps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="lift2SetReps"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lift2Name" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/at_symbol"
        app:layout_constraintStart_toEndOf="@+id/lift2SetReps"
        app:layout_constraintTop_toBottomOf="@+id/lift2Name" />

    <TextView
        android:id="@+id/lift2Weight"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="lift2Weight"
        app:layout_constraintEnd_toStartOf="@+id/lift2Reset"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/lift2Name" />

    <ImageButton
        android:id="@+id/lift2Complete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="16dp"
        android:onClick="liftCompleted"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lift1Complete"
        app:srcCompat="@drawable/ic_done_black_24dp" />

    <Button
        android:id="@+id/lift2Deets"
        android:layout_width="wrap_content"
        android:layout_height="55dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="16dp"
        android:onClick="liftDetails"
        android:text="@string/details_label"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/lift2Complete"
        app:layout_constraintTop_toBottomOf="@+id/lift1Deets" />

    <TextView
        android:id="@+id/lift3Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:text="lift3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lift2SetReps" />

    <TextView
        android:id="@+id/lift3SetReps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="life3SetReps"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lift3Name" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/at_symbol"
        app:layout_constraintStart_toEndOf="@+id/lift3SetReps"
        app:layout_constraintTop_toBottomOf="@+id/lift3Name" />

    <TextView
        android:id="@+id/lift3Weight"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="lift3Weight"
        app:layout_constraintEnd_toStartOf="@+id/lift3Reset"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/lift3Name" />

    <ImageButton
        android:id="@+id/lift3Complete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="16dp"
        android:onClick="liftCompleted"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lift2Complete"
        app:srcCompat="@drawable/ic_done_black_24dp" />

    <Button
        android:id="@+id/lift3Deets"
        android:layout_width="wrap_content"
        android:layout_height="55dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="16dp"
        android:onClick="liftDetails"
        android:text="@string/details_label"
        app:layout_constraintEnd_toStartOf="@+id/lift3Complete"
        app:layout_constraintTop_toBottomOf="@+id/lift2Deets" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:onClick="nextWorkout"
        android:text="@string/next_workout_label"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageButton
        android:id="@+id/lift1Reset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="16dp"
        android:onClick="liftReset"
        app:layout_constraintEnd_toStartOf="@+id/lift1Deets"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_replay_black_24dp" />

    <ImageButton
        android:id="@+id/lift2Reset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="16dp"
        android:onClick="liftReset"
        app:layout_constraintEnd_toStartOf="@+id/lift2Deets"
        app:layout_constraintTop_toBottomOf="@+id/lift1Reset"
        app:srcCompat="@drawable/ic_replay_black_24dp" />

    <ImageButton
        android:id="@+id/lift3Reset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="16dp"
        android:onClick="liftReset"
        app:layout_constraintEnd_toStartOf="@+id/lift3Deets"
        app:layout_constraintTop_toBottomOf="@+id/lift2Reset"
        app:srcCompat="@drawable/ic_replay_black_24dp" />

</android.support.constraint.ConstraintLayout>